  "main": "lib/index.js",
  "scripts": {
    "start": "npm run docs-dev",
    "docs-build": "node build/build.js",
    "docs-dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "lint": "eslint --ext .js,.vue docs --fix --debug -o ./test/lint.html -f html",
    "build": "npm run babelify",
    "test": "npm run babelify && npm run tape",
    "tape": "tape lib/test/index.js | tap-spec",
    "babelify": "babel --no-babelrc --presets=env,stage-2 --plugins=transform-runtime src --out-dir lib"
  },
